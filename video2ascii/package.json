  },
  "homepage": "https://github.com/chipoto69/vidas#readme",
  "bugs": {
    "url": "https://github.com/chipoto69/vidas/issues"
  },
  "keywords": [
    "ascii",
    "ascii-art",
    "video",
    "webgl",
    "webgl2",
    "react",
    "canvas",
    "shader",
    "glsl",
    "real-time",
    "graphics",
    "visualization",
    "creative-coding",
    "generative-art"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
